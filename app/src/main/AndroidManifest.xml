    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.INTERNET" />



    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".ffwc"></activity>
        <activity android:name=".bfs" />
        <activity android:name=".bdrcs" />
        <activity android:name=".ddm" />
        <activity android:name=".accidents" />
        <activity android:name=".fire_accident" />
        <activity
            android:name=".lesson"
            android:parentActivityName=".second" />
        <activity
            android:name=".donation"
            android:parentActivityName=".second" />
        <activity
            android:name=".massage"
            android:parentActivityName=".second" />

        <activity android:name =".second">
            <intent-filter>

                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>